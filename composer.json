{
    "name": "epwt/array-jms-serializer-bundle",
    "type": "symfony-bundle",
    "description": "JMS/SerializerBundle extension to serialize & deserialize to array format",
    "keywords": ["serialization", "deserialization", "array", "jms serializer", "jms serializer-bundle", "serializer-bundle"],
    "homepage": "https://bitbucket.org/epwt/array-jms-serializer-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Aurimas Niekis",
            "email": "aurimas.niekis@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "jms/serializer": ">=1.0",
        "jms/serializer-bundle": ">=1.0",
        "symfony/framework-bundle": ">=2.1"
    },
    "autoload": {
        "psr-0": { "EPWT\\ArrayJMSSerializerBundle": "" }
    },
    "target-dir": "EPWT/ArrayJMSSerializerBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
